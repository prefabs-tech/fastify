{
  "name": "@prefabs.tech/fastify-s3",
  "version": "0.93.4",
  "description": "Fastify S3 plugin",
  "homepage": "https://github.com/prefabs-tech/fastify/tree/main/packages/s3#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prefabs-tech/fastify.git",
    "directory": "packages/s3"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/prefabs-tech-fastify-s3.js",
      "require": "./dist/prefabs-tech-fastify-s3.cjs"
    }
  },
  "main": "./dist/prefabs-tech-fastify-s3.cjs",
  "module": "./dist/prefabs-tech-fastify-s3.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly && mv dist/src dist/types",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "sort-package": "npx sort-package-json",
    "test": "vitest run --coverage",
    "typecheck": "tsc --noEmit -p tsconfig.json --composite false"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.917.0",
    "@aws-sdk/lib-storage": "3.917.0",
    "@aws-sdk/s3-request-presigner": "3.917.0",
    "@fastify/multipart": "9.3.0",
    "@types/busboy": "1.5.4",
    "@types/uuid": "9.0.8",
    "ajv": "8.17.1",
    "busboy": "1.6.0",
    "graphql-upload-minimal": "1.6.4",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@prefabs.tech/eslint-config": "0.5.0",
    "@prefabs.tech/fastify-config": "0.93.4",
    "@prefabs.tech/fastify-error-handler": "0.93.4",
    "@prefabs.tech/fastify-graphql": "0.93.4",
    "@prefabs.tech/fastify-slonik": "0.93.4",
    "@prefabs.tech/tsconfig": "0.5.0",
    "@types/node": "24.10.13",
    "@vitest/coverage-istanbul": "3.2.4",
    "eslint": "9.39.2",
    "fastify": "5.6.2",
    "fastify-plugin": "5.1.0",
    "graphql": "16.11.0",
    "prettier": "3.6.2",
    "slonik": "46.8.0",
    "typescript": "5.9.3",
    "vite": "6.4.1",
    "vitest": "3.2.4",
    "zod": "3.25.76"
  },
  "peerDependencies": {
    "@prefabs.tech/fastify-config": "0.93.4",
    "@prefabs.tech/fastify-error-handler": "0.93.4",
    "@prefabs.tech/fastify-graphql": "0.93.4",
    "@prefabs.tech/fastify-slonik": "0.93.4",
    "fastify": ">=5.2.1",
    "fastify-plugin": ">=5.0.1",
    "slonik": ">=46.1.0",
    "zod": ">=3.23.8"
  },
  "engines": {
    "node": ">=20"
  }
