    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-unicorn": "56.0.1",
    "fastify": "5.6.1",
    "fastify-plugin": "5.1.0",
    "prettier": "3.6.2",
    "typescript": "5.9.3",
    "vite": "6.4.1",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "fastify": ">=5.2.1",
    "fastify-plugin": ">=5.0.1"
